<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#171f21" 
    android:layout_gravity="center"
    >

    <ScrollView
        android:id="@+id/visu_moyen_scroll"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_above="@+id/buttonDemande"
        android:layout_alignParentRight="true"
        android:scrollbars="none" 
        >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/inc"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#b5181d"
                        android:gravity="center"
                        android:text="@string/secteur_inc"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_inc"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/inc_lieux"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/inc_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/sap"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#469e2f"
                        android:gravity="center"
                        android:text="@string/secteur_sap"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_sap"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/sap_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/sap_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/alim"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#1085ad"
                        android:gravity="center"
                        android:text="@string/secteur_alim"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_alim"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/alim_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/alim_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/fdf"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_fdf"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_fdf"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/fdf_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/fdf_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/lif"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_lif"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_lif"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/lif_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/lif_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/chim"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_chime"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_chime"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/chime_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/chime_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/rad"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_rad"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_rad"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/rad_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/rad_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/sd"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_sd"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_sd"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/sd_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/sd_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="400dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/sr"
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginLeft="20dp"
                        android:layout_marginTop="20dp"
                        android:background="#878181"
                        android:gravity="center"
                        android:text="@string/secteur_sr"
                        android:textColor="#efefef"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/info_sr"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="20dp"
                        android:textColor="#efefef"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/sr_lieu"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/check"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/sr_parti"
                        android:layout_width="match_parent"
                        android:layout_height="33dp"
                        android:layout_marginLeft="30dp"
                        android:drawableLeft="@drawable/cross"
                        android:gravity="center_vertical"
                        android:textColor="#efefef"
                        android:textSize="16sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

    <Button
        android:id="@+id/buttonDemande"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="20dp"
        android:text="@string/demander_moyen"
        android:background="@drawable/red_button"
        android:textColor="#efefef" 
        android:padding="7dip"/>

</RelativeLayout>